*{font-family:Arial, Helvetica, Bitstream Vera Sans;font-size:x-small;margin:0;padding:0;}
html{width:100%;height:100%;margin:0;padding:0;}
body{background-color:#fff;border:0;height:100%;width:100%;text-align:center;vertical-align:middle;}
img,a{border:0;font-family:Arial;}
/*BordaLinksClicados*/*:focus{-moz-outline-style:none;}
#conteudo{height:78%;width:100%;text-align:left;padding:20px 0 0;}
.janela{text-align:center;vertical-align:middle;line-height:150%;position:absolute;z-index:31;padding:20px 10px 10px;}
.close{float:left;width:10px;height:10px;margin-right:5px;border:solid #000 0;}
.cortina{position:absolute;background-color:#fff;z-index:30;height:100%;width:100%;filter:alpha(opacity=50);opacity:0.5;}
.popup{background-color:#ff0;z-index:50;height:30%;width:25%;margin-left:25%;margin-top:10%;}
.thumb{width:80px;height:80px;}
#msgerro{padding-top:5px;color:red;}
/*AutoComplete*/.auto_complete{position:absolute;width:50mm;background-color:#FFF;border:1px solid #888;margin:0;padding:0;}
.auto_complete ul{list-style-type:none;margin:0;padding:0;}
.auto_complete ul li.selected{background-color:#FF0;}
.auto_complete ul li{list-style-type:none;display:block;height:16px;margin:0;padding:2px;}
/*BarraMenu*/
#barra{background-color:#696969;position:absolute;height:30px;width:100%;margin-top:15px;bottom:6%;left:0;margin-bottom:15px;vertical-align:bottom;}
#barra ul{font-size:12px;font-weight:700;list-style:none;margin:0 100px 0 60px;padding:0;}
#barra li{display:inline;margin:0;padding:0;height: 100%;}
.menu_item a{float:left;display:block;background-color:transparent;padding:4px 8px;}
.menu_item a:hover{background-color:#FFF;color:#fff;position:relative;}
.menu_item a span{display:none;font-weight:400;font-size:x-small;}
.menu_item a:hover span{color:#000;display:block;position:absolute;background-color:transparent;top:-15px;left:0;width:600px;letter-spacing:0;text-align:left;}
.admin a{float:right;display:block;color:#FF0;text-decoration:none;letter-spacing:1px;background-color:#696969;border-left-style:solid;border-left-width:1px;border-left-color:#999;margin:0 1px 0 0;padding:4px 8px;}
.admin a:hover{background-color:#000;}
.home a{float:right;display:block;margin:0 1px 0 0;background-color:transparent;padding:4px 8px;}
#status li{float:right;display:block;color:#FFF;font-weight:700;font-size:small;letter-spacing:1px;margin:7px;}
#rodape{left:0;position:absolute;width:100%;height:1em;bottom:4%;font-size:9pt;vertical-align:middle;}
.arroba{vertical-align:middle;}
/*Foto*/#div_foto{float:left;width:100%;height:100%;text-align:left;}
#div_foto img{height:100%;width:100%;}
/*Logo*/#logo{position:absolute;left:100%;top:3.4%;margin-left:-200px;z-index:10;}
#logo_image{width:100%;}
/*Admin*/#admin{position:relative;display:inline;width:50%;}
.mini{height:15%;width:25%;margin-left:37.5%;margin-top:15%;}
/*Login*/#auth_label{float:left;width:120px;margin-bottom:15px;font-weight:700;}
.login_input{width:180px;margin-bottom:5px;}
#submitbutton{margin-left:120px;margin-top:5px;width:90px;}
br{clear:left;}
/*TrocaFoto*/.invisible{width:1px;height:1px;border:0;}
.normal{border:0 dashed #000;}
/*Maps*/#mapa_div{position:absolute;left:0;width:100%;height:78%;text-align:left;}
.tiny{position:relative;margin-right:20px;vertical-align:middle;max-width:40px;max-height:40px;}
